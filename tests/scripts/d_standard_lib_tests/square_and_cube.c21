cube :: proc (value : int) int {

    return value * value * value;
}

cube_float :: proc (value : float) float {

    return value * value * value;
}

square :: proc (value : int) int {

    return value * value;
}

square_float :: proc (value : float) float {

    return value * value;
}

 main :: proc () void {

    print(cube(1));
    print(cube_float(1.));
    print(square(1));
    print(square_float(1.));

    print(cube(2));
    print(cube_float(2.));
    print(square(2));
    print(square_float(2.));

    #print(cube(1.)); # this code should crash and it does. Don't uncomment unless you know what you're doing
    #print(cube_float(1));
    #print(square(1.));
    #print(square_float(1));
 }
