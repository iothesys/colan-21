celsius_to_farenheit :: (celsius : float) float {

    return celsius * 1.8 + 32.;
}

celsius_to_kelvin :: (celsius : float) float {

    return celsius + 273.15;
}

farenheit_to_celsius :: (farenheit : float) float {

    return (farenheit - 32.) * (5. / 9.);
}

farenheit_to_kelvin :: (farenheit : float) float {

    return (farenheit + 459.67) * (5. / 9.);
}

kelvin_to_celsius :: (kelvin : float) float {

    return kelvin âˆ’ 273.15;
}

kelvin_to_farenheit :: (kelvin : float) float {

    return kelvin * 1.8 - 459.67;
}

millimeter_to_inch :: (millimeter : float) float {

    return millimeter * 5. / 127.;
}

inch_to_millimeter :: (inch : float) float {

    return inch * 25.4;
}

meter_to_foot :: (meter : float) float {

    return meter * 5000. / 127.;
}

foot_to_meter :: (foot : float) float {

    return foot * .3048;
}

meter_to_yard :: (meter : float) float {

    return meter * 1250. / 1143.;
}

yard_to_meter :: (yard : float) float {

    return yard * 0.9144;
}

kilometer_to_mile :: (kilometer : float) float {

    return kilometer * .621;
}

mile_to_kilometer :: (mile : float) float {

    return mile * 1.609344;
}

kilometer_to_nautical_mile :: (kilometer : float) float {

    return kilometer * .540;
}

nautical_mile_to_kilometer :: (nautical_mile : float) float {

    return nautical_mile * 1.852;
}
